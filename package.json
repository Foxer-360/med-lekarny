    "watch": "npx gulp watch",
    "watch:js": "npx gulp watchJs",
    "watch:sass": "npx gulp watchSass"
  },
  "peerDependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/google-map-react": "^0.23.10",
    "@types/lodash": "^4.14.119",
    "@types/node": "^10.12.12",
    "@types/prop-types": "^15.5.6",
    "@types/react": "*",
    "@types/react-dom": "16.0.11",
    "@types/react-router": "^5.0.0",
    "@types/react-router-dom": "^4.3.0",
    "@types/react-test-renderer": "^16.8.2",
    "@types/zen-observable": "^0.8.0",
    "autoprefixer": "^9.5.1",
    "babel-jest": "^24.8.0",
    "eslint": "^5.16.0",
    "gulp": "^4.0.2",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.0.2",
    "jest": "^24.8.0",
    "react-test-renderer": "^16.8.6",
    "ts-jest": "^24.0.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "@types/jest": "^24.0.15",
    "apollo-boost": "^0.1.22",
    "apollo-client": "^2.4.7",
    "bootstrap": "^4.1.3",
    "google-map-react": "^1.1.2",
    "graphql": "0.13.0",
    "graphql-tag": "^2.9.2",
    "lodash": "^4.17.4",
    "moment": "^2.23.0",
    "obj-traverse": "^1.0.0",
    "react-adopt": "^0.6.0",
    "react-apollo": "2.5.1",
    "react-cookie-consent": "^2.3.1",
    "react-geocode": "^0.1.2",
    "react-geolocated": "^2.4.0",
    "react-lines-ellipsis": "^0.13.2",
    "react-markdown": "^4.0.4",
    "react-masonry-css": "^1.0.11",
    "react-responsive": "^6.1.1",
    "react-router-dom": "^4.3.1",
    "react-slick": "^0.24.0",
    "react-swipeable": "^4.3.0",
    "slick-carousel": "^1.8.1"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "coveragePathIgnorePatterns": ["/node_modules/", "/build/"],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    },
    "moduleNameMapper": {
      "@source/(.*)": "<rootDir>/src/$1"
    }
  }
