  "license": "MIT",
  "scripts": {
    "build": "npx gulp build",
    "build:js": "npx gulp buildJs",
    "build:sass": "npx gulp buildSass",
    "clean": "rm -rf build && rm styles/main.css",
    "hot": "yarn watch",
    "up": "yarn install --check-files",
